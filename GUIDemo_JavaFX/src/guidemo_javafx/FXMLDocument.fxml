<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo_javafx.FXMLDocumentController">
   <children>
      <HBox prefHeight="250.0" prefWidth="694.0">
         <children>
            <VBox prefHeight="279.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="243.0" text="Checkbox Objects">
                     <font>
                        <Font name="System Bold Italic" size="13.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="Pineapple" />
                  <CheckBox fx:id="beaconCheckBox" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#pizzaOderButtonPushed" text="Order Pizza" />
                  <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="132.0" prefWidth="213.0" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox prefHeight="279.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Label text="ChoiceBox Objects">
                     <font>
                        <Font name="System Bold Italic" size="13.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxPushed" text="Favourite Fruits" />
                  <Label fx:id="choiceBoxLabel" alignment="TOP_LEFT" contentDisplay="CENTER" prefHeight="98.0" prefWidth="154.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="279.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Label text="ComboBox">
                     <font>
                        <Font name="System Bold Italic" size="13.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdated" prefWidth="150.0" />
                  <Label fx:id="comboBoxLabel" alignment="TOP_LEFT" prefHeight="83.0" prefWidth="149.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="279.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Label text="RadioBtn Objects">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="javaRadioBtn" mnemonicParsing="false" onAction="#radioButtonChanged" text="JAVA" />
                  <RadioButton fx:id="cSharpRadioBtn" mnemonicParsing="false" onAction="#radioButtonChanged" text="C#" />
                  <RadioButton fx:id="aspRadioBtn" mnemonicParsing="false" onAction="#radioButtonChanged" text="ASP.NET" />
                  <RadioButton fx:id="phpRadioBtn" mnemonicParsing="false" onAction="#radioButtonChanged" text="PHP" />
                  <Label fx:id="radioButtonLabel" alignment="TOP_LEFT" prefHeight="116.0" prefWidth="159.0" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <HBox layoutX="3.0" layoutY="248.0" prefHeight="195.0" prefWidth="687.0" spacing="5.0">
         <children>
            <ListView prefHeight="198.0" prefWidth="153.0" />
            <StackPane prefHeight="198.0" prefWidth="165.0">
               <children>
                  <Button mnemonicParsing="false" text="Selected Gear" />
               </children>
            </StackPane>
            <TextArea prefHeight="198.0" prefWidth="176.0" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</AnchorPane>

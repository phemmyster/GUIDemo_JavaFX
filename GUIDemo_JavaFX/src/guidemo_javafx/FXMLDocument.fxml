<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="guidemo_javafx.FXMLDocumentController">
   <children>
      <VBox prefHeight="400.0" prefWidth="425.0" spacing="10.0">
         <children>
            <Label prefHeight="27.0" prefWidth="243.0" text="Checkbox Objects">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <CheckBox fx:id="pepperoniCheckBox" layoutX="95.0" layoutY="83.0" mnemonicParsing="false" text="Pepperoni" />
      <CheckBox fx:id="pineappleCheckBox" layoutX="95.0" layoutY="118.0" mnemonicParsing="false" text="Pineapple" />
      <CheckBox fx:id="beaconCheckBox" layoutX="95.0" layoutY="151.0" mnemonicParsing="false" text="Bacon" />
      <Button layoutX="96.0" layoutY="188.0" mnemonicParsing="false" onAction="#pizzaOderButtonPushed" text="Order Pizza" />
      <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" layoutX="96.0" layoutY="241.0" prefHeight="54.0" prefWidth="182.0" text="Label" />
   </children>
</AnchorPane>
